
func main returns f64
    arg arg_n i64
    stack_slot sum 8
    stack_slot flip 8
    stack_slot i 8
    stack_slot n 8
    store sum 0i64
    store flip -1.0f64
    store i 1i64
    store n 100000000i64
    goto loop_head
block loop_head
    nval = load i64 n
    ival = load i64 i
    
    f = load f64 flip
    f2 = fmul f -1.0f64
    store flip f2
    
    //a = mul ival 2i64
    a = shl ival 1i64
    b = sub a 1i64
    c = uint_to_float f64 b
    d = fdiv f2 c
    
    s = load f64 sum
    s2 = fadd s d
    store sum s2
    
    ival2 = add ival 1i64
    
    store i ival2
    
    cmp = cmp_g ival2 nval
    if cmp goto out
    
    goto loop_head
block out
    ret = load f64 sum
    ret2 = fmul ret 4.0f64
    return ret2
endfunc
