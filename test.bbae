
func main returns f64
    arg arg_n i64
    stack_slot sum 8
    stack_slot flip 8
    stack_slot i 8
    stack_slot n 8
    store sum 0i64
    store flip -1.0f64
    store i 1i64
    store n arg_n
    goto loop_head
block loop_head
    nval = load i64 n
    ival = load i64 i
    cmp = cmp_g ival nval
    if cmp goto out
    
    f = load f64 flip
    f2 = fmul f -1.0f64
    store flip f2
    
    a = mul ival 2i64
    b = sub a 1i64
    c = uint_to_float f64 b
    d = fdiv f2 c
    
    s = load f64 sum
    s2 = add s d
    store s s2
    goto loop_head
block out
    ret = load f64 sum
    return ret
endfunc
